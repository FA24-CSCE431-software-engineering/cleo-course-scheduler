<h1>Prerequisites</h1>

<table>
  <thead>
    <tr>
      <th>Course ID</th>
      <th>Prerequisite ID</th>
      <th>Equivalent ID</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @prerequisites.each do |prerequisite| %>
      <tr>
        <td><%= prerequisite.course_id %></td>
        <td><%= prerequisite.prereq_id %></td>
        <td><%= prerequisite.equi_id %></td>
        <td><%= link_to 'Show', admin_prerequisite_path(prerequisite) %></td>
        <td><%= link_to 'Edit', edit_admin_prerequisite_path(prerequisite) %></td>
        <td><%= link_to 'Delete', confirm_destroy_admin_prerequisite_path(prerequisite), method: :get %></td>
        
      </tr>
    <% end %>
  </tbody>
</table>

<%= link_to 'New Prerequisite', new_prerequisite_path %>
