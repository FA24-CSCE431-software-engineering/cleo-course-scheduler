<h1>Students</h1>

<table>
  <thead>
    <tr>
      <th>UIN</th>

      <th>First Name</th>

      <th>Last Name</th>

      <th>Email</th>

      <th>Enrollment Year</th>     

      <th>Graduation Year</th> 

      <th>Enrollment Semester</th> 

      <th>Graduation Semester</th> 

      <th>Actions</th>
    </tr>
  </thead>
  <tbody>
    <% @students.each do |student| %>
      <tr>
        <td><%= student.uin %></td>

        <td><%= student.first_name %></td>

        <td><%= student.last_name %></td>

        <td><%= student.email %></td>

        <td><%= student.enrol_year %></td>

        <td><%= student.grad_year %></td>

        <td><%= student.enrol_semester %></td>

        <td><%= student.grad_semester %></td>

        <td>
          <%= link_to 'UPDATE', edit_student_path(student) %>
          <%= link_to 'DELETE', student_path(student), method: :delete, data: { confirm: 'Are you sure?' } %>
        </td>
      </tr>
    <% end %>
  </tbody>
</table>

<div>
  <%= link_to 'CREATE', new_student_path %>
</div>
<div>
  <%= link_to 'Back to Dashboard', root_path %>
</div>
